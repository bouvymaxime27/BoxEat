{
  "name": "boxeat",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "EXPO_NO_TELEMETRY=1 EXPO_USE_DEV_SERVER=true npx expo start --tunnel --clear",
    "web": "EXPO_DEVTOOLS_LISTEN_ADDRESS=0.0.0.0 npx expo start --web --port 5000",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "reset": "rm -rf node_modules package-lock.json .expo .cache && npm install --legacy-peer-deps"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.17",
    "axios": "^1.7.0",
    "expo": "~54.0.0",
    "firebase": "^12.4.0",
    "react": "18.2.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "babel-preset-expo": "^10.0.0"
  }
}

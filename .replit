# .replit
entrypoint = "ind.⁷nex.js"

[env]
EXPO_NO_TELEMETRY = "1"
EXPO_USE_DEV_SERVER = "true"
EXPO_DEVTOOLS_LISTEN_ADDRESS = "0.0.0.0"

[interpreter]
command = ["bash", "-lc"]

# Lancement par défaut du repl
run = "npm ci || npm install; npx expo start --tunnel --clear"

[agent]
expertMode = true
